              <script>
              $(function() {
                $("#datepicker1, #datepicker2").datepicker({
                  dateFormat: 'yy.mm.dd'
                });
              });

              </script>






              <div class="row">
                <div class="col-lg-3 col-sm-4">
                  <h5 class="text-uppercase mb-2 text-center">스터디룸 이름</h5>
                </div>
                <div class="col-lg-8 col-sm-8">
                  <input type="text" class="form-control" name="facilityname" value="<%= curFacilityname%>" readonly>
                </div>
              </div>

              <hr>



              <div class="row">
                <div class="col-lg-3 col-sm-2">
                  <h5 class="text-uppercase mb-2 text-center">예약 시간</h5>
                </div>
                <div class="col-lg-2 col-sm-2">
                  <input type="text" class="form-control" name="date" id="datepicker1">
                </div>
                <div class="col-lg-2 col-sm-2">
                  <input type="text" class='timepicker form-control' name="starttime" placeholder="ex) 7:00">
                </div>
                <div class="col-lg-1 col-sm-1">
                  <h5><center>
                    부터
                  </center></h5>
                </div>
                <div class="col-lg-2 col-sm-2">
                  <input type="text" class='timepicker form-control' name="endtime" placeholder="ex) 10:00">
                </div>
              </div>
              <hr>

              <div class="row">
                <div class="col-lg-3 col-sm-4">
                  <h5 class="text-uppercase mb-2 text-center">예약자 이름</h5>
                </div>
                <div class="col-lg-2 col-sm4">
                  <input type="text" class="form-control" name="username" value="<%=user_name%>" readonly>
                </div>
                <div class="col-lg-3 col-sm-4">
                  <h5 class="text-uppercase mb-2 text-center">예약자 전화번호</h5>
                </div>
                <div class="col-lg-2 col-sm4">
                  <input type="text" class="form-control" name="phone" value="<%=user_phone%>" readonly>
                </div>
              </div>
              <hr>
              <br>

              <div class="row">
                <div class="col-lg-11 col-sm-10">
