      <script src="/public/bootstrap/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
      <script src="/public/bootstrap/vendor/jquery-easing/jquery.easing.min.js"></script>
      <script src="/public/bootstrap/vendor/magnific-popup/jquery.magnific-popup.min.js"></script>
      <script src="/public/bootstrap/js/jqBootstrapValidation.js"></script>
      <script src="/public/bootstrap/js/contact_me.js"></script>
      <script src="/public/bootstrap/js/freelancer.min.js"></script>
      <script>
      function listpost(page, perPage) {
        document.location = '/user/search?page=' + page + '&perPage=' + perPage;
      }
      </script>
    </body>

    </html>
